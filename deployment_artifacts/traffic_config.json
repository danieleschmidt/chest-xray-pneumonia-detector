{
  "load_balancer": {
    "algorithm": "round_robin",
    "health_check_enabled": true,
    "sticky_sessions": false
  },
  "traffic_split": {
    "blue": 0,
    "green": 100
  },
  "rate_limiting": {
    "enabled": true,
    "requests_per_minute": 1000,
    "burst_limit": 100
  },
  "cdn": {
    "enabled": true,
    "cache_static_assets": true,
    "cache_ttl": 3600
  }
}